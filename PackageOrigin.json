    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "winston": "^3.11.0",
    "@octokit/rest": "^20.0.2",
    "node-cron": "^3.0.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^4.0.1",
    "uuid": "^9.0.1"
